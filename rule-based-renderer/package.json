{
  "name": "react-rule-based-renderer",
  "version": "0.0.1",
  "description": "A react rule-based renderer, useful on generating dynamic forms",
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/narixius/",
  "bugs": {
    "url": "https://github.com/Narixius/react-rule-based-renderer/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Narixius/react-rule-based-renderer.git"
  },
  "author": "Nariman Movaffaghi",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./types": {
      "types": "./dist/types.d.ts",
      "default": "./dist/types.js"
    },
    "./resolver": {
      "types": "./dist/resolver.d.ts",
      "default": "./dist/resolver.js"
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && pnpm publish",
    "prepublishOnly": "pnpm run build"
  },
  "peerDependencies": {
    "@emotion/react": ">=11",
    "@emotion/styled": ">=11.14.1",
    "@hookform/resolvers": ">=5",
    "@mui/icons-material": ">=7",
    "@mui/material": ">=7",
    "es-toolkit": ">=1.39",
    "react": ">= 19",
    "react-dom": ">= 19",
    "react-hook-form": "=7.62.0",
    "yup": ">=1.7.0"
  },
  "devDependencies": {
    "@sxzz/eslint-config": "^7.1.4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^22.15.17",
    "@types/react": "^19.1.3",
    "@types/react-dom": "^19.1.4",
    "bumpp": "^10.1.0",
    "eslint": "^9.33.0",
    "happy-dom": "^17.4.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tsdown": "^0.11.1",
    "typescript": "^5.8.3",
    "vitest": "^3.1.3"
  }
}
